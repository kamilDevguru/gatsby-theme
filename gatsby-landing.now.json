{
  "version": 2,
  "name": "gatsby-landing",
  "builds": [
    {
      "src": "packages/landing-gatsby/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/packages/landing-gatsby/$1",
      "headers": {
        "x-request-path": "$1"
      }
    }
  ]
}
